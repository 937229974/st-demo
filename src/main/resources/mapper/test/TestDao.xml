<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.st.test.dao.TestDao">


    <select id="queryInfo" resultType="com.st.test.pojo.Test" parameterType="java.lang.String">
		SELECT
			*
		FROM
			test AS t
		WHERE
			t.id = #{id}
	</select>


</mapper>